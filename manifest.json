{
  "name": "William Whitakers Words V.2",
  "description": "William Whitakers Words that works",
  "version": "0.1.1",
  "manifest_version": 3,

  "permissions": ["storage", "activeTab", "scripting", "notifications"],

  "content_scripts": [{
    "css": ["whitakers-words-style.css"],
    "matches": ["http://www.archives.nd.edu/*", "https://archives.nd.edu/cgi-bin/*"]
  }],

  "action": {
    "default_title": "William Whitakers Words V.2",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}
